package ws;

import java.util.List;

import javax.jws.WebService;

import api.GoogleBooksAPI;
import model.Book;

@WebService(endpointInterface = "ws.SearchService")
public final class SearchServiceImpl implements SearchService{

	@Override
	public ArrayList<Book> searchBooksByKeyword(String title) {
		return GoogleBooksAPI.getBooks(title);
	}

	@Override
	public Book getBookDetails(String id) {
		// TODO Auto-generated method stub
		return null;
	}
}
